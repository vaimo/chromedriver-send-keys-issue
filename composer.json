{
    "name": "vaimo/chromedriver-send-keys-issue",
    "type": "project",
    "license": "MIT",
    "description": "An example of some keys being eaten by the chrome-driver on Linux",
    "require": {
        "codeception/codeception": "^2.3",
        "tivie/php-os-detector": "^1.0",
        "consolidation/robo": "^1.1",
        "monolog/monolog": "^1.23",
        "vaimo/binary-chromedriver": "^2.35"
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "test": "bin/robo run:tests --ansi"
    },
    "autoload": {
        "psr-4": {
            "Vaimo\\ChromeDriverExample\\": "lib/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:vaimo/binary-chromedriver.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.org/"
        }
    ]
}
